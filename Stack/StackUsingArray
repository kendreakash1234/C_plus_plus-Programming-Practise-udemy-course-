#include <iostream>
using namespace std;

class Stack{
    
    int size;
    int top;
    int *arr;
    public:
    Stack(int n){
        size = n;
        arr = new int(size);
        top = -1;
    }
    void push(int n){
        if(top >= size-1)
            cout<<"Stack overflow, could not insert element "<<n<<endl;
        else{
            arr[++top] = n;
        }   
    }
    
    void pop(){
        if(top < 0)
            cout<<"Stack underflow"<<endl;
        else{
            top--;
        }
    }
    
    void display(){
        cout<<"Stack elements"<<endl;
        for(int i=top;i>=0;i--)
            cout<<arr[i]<<endl;
    }
};

int main()
{
    Stack s(6);
    s.push(3);
    s.push(5);
    s.push(6);
    s.push(8);
    s.push(9);
    s.push(4);
    s.push(1);
    // s.pop();
    s.display();
    
    return 0;
}